Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Slider", "[", 
     RowBox[{
      RowBox[{"Dynamic", "[", "a", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"0.1", ",", "2", ",", ".1"}], "}"}]}], "]"}], "}"}], ",", 
   RowBox[{"Dynamic", "[", 
    RowBox[{"StringJoin", "[", 
     RowBox[{"\"\<a = \>\"", ",", 
      RowBox[{"ToString", "[", "a", "]"}]}], "]"}], "]"}], ",", 
   RowBox[{"Dynamic", "[", 
    RowBox[{"Plot", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SuperscriptBox["x", "2"], ",", 
        FractionBox["1", 
         SuperscriptBox["x", "2"]]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "a", ",", "4.5"}], "}"}], ",", 
      RowBox[{"PlotLabels", "\[Rule]", 
       RowBox[{"Placed", "[", 
        RowBox[{
         RowBox[{"{", 
